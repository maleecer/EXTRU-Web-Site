# Environment Variables Template
# Copy this file to .env.local and update with your actual values

# Site URL - Update with your actual domain when deploying
NEXT_PUBLIC_SITE_URL=https://extru2026.com

# Google Search Console Verification (Get from Google Search Console)
# NEXT_PUBLIC_GOOGLE_VERIFICATION=your-verification-code-here

# Google Analytics (Optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Bing Webmaster Verification (Optional)
# NEXT_PUBLIC_BING_VERIFICATION=your-bing-verification-code

# Contact Email
NEXT_PUBLIC_CONTACT_EMAIL=info@extru2026.com

# Social Media URLs (Update with your actual social media handles)
NEXT_PUBLIC_FACEBOOK_URL=https://www.facebook.com/extru2026
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/extru2026
NEXT_PUBLIC_INSTAGRAM_URL=https://www.instagram.com/extru2026
NEXT_PUBLIC_LINKEDIN_URL=https://www.linkedin.com/company/extru2026
